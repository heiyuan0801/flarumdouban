{
    "name": "doubanapi/flarum-ext-douban",
    "description": "Add douban information search and insert to editor",
    "keywords": ["flarum", "douban"],
    "type": "flarum-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "flarum/core": "^1.0.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "authors": [
        {
            "name": "doubanapi",
            "email": "doubanapi@example.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Doubanapi\\Douban\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Douban Search",
            "category": "feature",
            "icon": {
                "name": "fas fa-search",
                "backgroundColor": "#238636",
                "color": "#fff"
            }
